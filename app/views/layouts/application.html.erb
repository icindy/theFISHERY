</head>
<body>

          <%= render 'layouts/navigation' %>
          <%= render 'layouts/header' %>
  
   
      <div class="container-fluid">
        
           <div class="row-fluid">

           <div class="span2"> 
              <%= render 'layouts/sidebar' %>
            </div> 
       


            <div class="span10">
              <%= render 'layouts/messages' %>
              <div class="hero-unit">
              <%= yield %>
              </div>
            </div>

         
         </div>
     </div> 
  
          <%= render 'layouts/footer' %>
